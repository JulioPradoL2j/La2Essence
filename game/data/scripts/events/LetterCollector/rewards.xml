<?xml version="1.0" encoding="UTF-8"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="rewards.xsd">
	<params>
		<minimum level="40" />
		<maximum level="-1" /> <!-- -1 = no limit -->
	</params>
	<letters>
		<!-- name of letter, for using down / id of this letter -->
		<item name="A" id="3875" />
		<item name="E" id="3877" />
		<item name="G" id="3879" />
		<item name="H" id="3880" />
		<item name="I" id="3881" />
		<item name="L" id="3882" />
		<item name="N" id="3883" />
		<item name="O" id="3884" />
		<item name="R" id="3885" />
		<item name="S" id="3886" />
		<item name="T" id="3887" />
		<item name="II" id="3888" />
		<item name="M" id="34956" />
		<item name="U" id="93413" />
	</letters>
	<!-- client id -->
	<reward id="0">
		<!-- split letter with ";" / HOT -->
		<word>H;O;T</word> <!-- HOT -->
		<!-- sumChances - sum item chances cumulatively -->
		<rewards sumChances="true">
			<!-- item id; count; chance; enchantLevel; / id="-1" = no reward -->
			<item id="104478" count="10" chance="100" /> <!-- Special H.O.T Word Reward Pack (Time-limited) Sealed -->
		</rewards>
	</reward>
	<reward id="1">
		<!-- split letter with ";" / SUMMER -->
		<word>S;U;M;M;E;R</word> <!-- SUMMER -->
		<rewards sumChances="true">
			<item id="104479" count="1" chance="100" /> <!-- Special S.U.M.M.E.R Word Reward Pack (Time-limited) Sealed -->
		</rewards>
	</reward>
	<reward id="2">
		<!-- split letter with ";" / LINEAGE II -> L;I;N;E;A;G;E;II -->
		<word>L;I;N;E;A;G;E;II</word> <!-- LINEAGE II -->
		<rewards sumChances="true">
			<item id="103250" count="1" chance="100" /> <!-- L.I.N.E.A.G.E.2 Word Special Reward Pack (Time-limited) Sealed -->
		</rewards>
	</reward>
</list>